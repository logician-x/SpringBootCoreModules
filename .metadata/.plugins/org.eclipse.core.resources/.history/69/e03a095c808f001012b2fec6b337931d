package com.ng;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.context.ConfigurableApplicationContext;

import com.nt.sbeans.Cricketer;

@SpringBootApplication
public class SpringProj02Application {

	public static void main(String[] args) {
		try(ConfigurableApplicationContext ctx = SpringApplication.run(SpringProj02Application.class, args);){
			
				Cricketer cricketer = ctx.getBean(Cricketer.class);
				cricketer.ball("Nageshwar");
				cricketer.batting("Manmath");
		}
		catch(Exception e) {
			e.printStackTrace();
		}
	}

}
