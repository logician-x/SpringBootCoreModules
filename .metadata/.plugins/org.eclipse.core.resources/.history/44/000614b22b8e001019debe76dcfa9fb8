package com.ng;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.context.ConfigurableApplicationContext;

import com.ng.sbeans.NumberService;

@SpringBootApplication
public class EvenOrOddCheckerApplication {

	public static void main(String[] args) {
			
			try(ConfigurableApplicationContext ctx =SpringApplication.run(EvenOrOddCheckerApplication.class, args)){
				
				  NumberService nservic = ctx.getBean(NumberService.class);
				 int a = 10;
				 switch(a) {
				 
				  case 1-5:System.out.println("15");break;
				  case 6-12:System.out.println("12");break;
				    default:System.out.println("default");
				 }
				
			}
				 catch(Exception e) {
				
				e.printStackTrace();
			}
		
	}
	

}
